<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weikbest.pro.saas.merchat.prod.mapper.ProdComboMapper">

    <select id="getIdByProdIdAndSetMealType" resultType="java.lang.Long">
        SELECT id FROM t_mmdm_prod_combo WHERE prod_id = #{prodId} AND set_meal_type = #{setMealType}  AND flag = 0
    </select>

</mapper>

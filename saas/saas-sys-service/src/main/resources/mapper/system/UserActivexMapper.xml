<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weikbest.pro.saas.sys.system.mapper.UserActivexMapper">

    <update id="disabledByIds">
        update t_sys_user_activex set activex_data_status = '0'
        where id in
        <foreach collection="idList" item="id" index="idx" open="(" separator="," close=")">
            #{id}
        </foreach>
    </update>
</mapper>
